config contact "Contact"
    option nickname ''
    option realname ''
    option email ''
    option contact_url ''
    option password ''
    option repeatpassword ''

config router_config "routerconfig"
    option community ''
    option nodename ''
    option location ''
    option lat ''
    option lon ''
    option share_internet ''
    option mesh_via ''
    option download_bandwidth ''
    option upload_bandwidth ''
    option auto_bandwidth ''
    option monitoring ''
    option autoupdate ''
    list ipv4_mesh_ips ''
    list ipv4_dhcp ''
    list ipv6_ips ''
